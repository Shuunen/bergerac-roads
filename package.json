{
  "name": "bergerac-roads",
  "version": "1.0.0",
  "description": "A great project",
  "engines": {
    "node": ">=10.15.0"
  },
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "analyze": "nuxt build --analyze",
    "dev": "nuxt",
    "dev:import": "nodemon --exec babel-node data/import.js --watch data/import.js",
    "build": "npm-run-all build:data build:app",
    "build:data": "npm run data:import",
    "build:app": "nuxt build --spa",
    "start": "nuxt start --spa",
    "data:db": "json-server data/db.json",
    "data:import": "babel-node data/import.js",
    "data:sample": "babel-node data/sample.js",
    "data:sprite": "babel-node data/sprite.js",
    "generate": "nuxt generate --spa",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "npm-run-all --parallel dev:* test:*",
    "test:panel": "./node_modules/.bin/cypress open",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/pwa": "2.6.0",
    "axios": "0.18.0",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.6",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "element-ui": "2.7.2",
    "fast-deep-equal": "2.0.1",
    "json-server": "0.12.2",
    "lodash": "4.17.11",
    "node-sass": "4.11.0",
    "npm-run-all": "4.1.5",
    "nuxt": "1.4.5",
    "nuxt-sass-resources-loader": "2.0.5",
    "sass-loader": "7.1.0",
    "speakingurl": "14.0.1",
    "style-loader": "0.21.0",
    "vue-i18n": "7.8.1",
    "vue-lazyload": "1.2.6",
    "vue-multiselect": "2.1.4",
    "vue2-google-maps": "0.9.8"
  },
  "devDependencies": {
    "@types/node-sass": "3.10.32",
    "eslint": "4.19.1",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-vue": "5.2.2",
    "glob-all": "3.1.0",
    "is-installed": "2.0.1",
    "node-sprite-generator": "0.10.2",
    "nodemon": "1.18.11",
    "stylelint-config-standard": "18.2.0"
  }
}
