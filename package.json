{
  "name": "bergerac-roads",
  "version": "1.0.0",
  "description": "A great project",
  "engines": {
    "node": ">=9.0.0"
  },
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build --spa",
    "start": "nuxt start --spa",
    "data": "npm-run-all --parallel data:*",
    "data:db": "json-server data/db.json",
    "data:import": "nodemon --exec babel-node data/import.js --watch data/*.js --ignore data/sample.js",
    "data:sample": "nodemon --exec babel-node data/sample.js --watch data/*.js --ignore data/import.js",
    "generate": "nuxt generate --spa",
    "lint": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "test": "npm-run-all --parallel dev:* test:*",
    "test:panel": "./node_modules/.bin/cypress open",
    "precommit": "npm run lint"
  },
  "devDependencies": {
    "@nuxtjs/pwa": "^2.0.8",
    "@types/node-sass": "^3.10.32",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-preset-env": "^1.7.0",
    "cypress": "^2.1.0",
    "element-ui": "^2.3.9",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.5.0",
    "fast-deep-equal": "^2.0.1",
    "glob-all": "^3.1.0",
    "json-server": "^0.12.2",
    "lodash": "^4.17.10",
    "lodash.clonedeep": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "lodash.samplesize": "^4.2.0",
    "lodash.trimstart": "^4.5.1",
    "lodash.truncate": "^4.4.2",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "nuxt": "^1.4.0",
    "nuxt-sass-resources-loader": "^2.0.0",
    "sass-loader": "^7.0.1",
    "speakingurl": "^14.0.1",
    "style-loader": "^0.21.0",
    "stylelint-config-standard": "^18.2.0",
    "vue-i18n": "^7.7.0",
    "vue-lazyload": "^1.2.2",
    "vue-multiselect": "^2.1.0",
    "vue2-google-maps": "^0.9.7"
  }
}
